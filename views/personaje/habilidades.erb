<table id='tresp'>
	<thead>
		<tr>
			<% clases.keys.each do |pj| %>
				<th width='12%'><%= pj.capitalize %></th>
			<% end %>
		</tr>
	</thead>
	<tbody>
		<tr>
			<% clases.keys.each do |clase| %>
				<td><img src=../images/personajes/<%= clase %>.png alt="<%= clase %>" /, id='icon2'></td>
			<% end %>
		</tr>
		<% 3.times do |i| %>
			<tr>
				<% clases.values.each do |clase| %>
					<td>
						<% h = clase[i] %>
						<a href="/habilidad/<%= h %>">
						<%= h %><br>
						<img src="/images/clases/<%= h %>.png" alt=<%= h %> /, id='icon3'></a>
					</td>
				<% end %>
			</tr>	
		<% end %>
	</tbody>
</table>