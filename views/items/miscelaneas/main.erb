<link rel="stylesheet" href="../css/bravo.css" />
<section>
	<table id='tresp'>
		<thead>
			<tr>
				<th>Icono</th>
				<th>Nombre</th>
				<th>Permitidos</th>
			</tr>	
		</thead>
		<tbody>
			<% miscelaneas.each do |misc| %>
			<% m = Miscelanea.new(misc)%>
				<tr>
					<td><img src=<%= m.img_path %> alt=<%= m.name %>, id='icon' /></td>
					<td><%= m.name %></td>
					<td><%= m.max %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</section>